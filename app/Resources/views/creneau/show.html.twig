{% extends 'base.html.twig' %}

{% block body %}
    <h1>Creneau</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ creneau.id }}</td>
            </tr>
            <tr>
                <th>Creneaudbt</th>
                <td>{% if creneau.creneauDbt %}{{ creneau.creneauDbt|date('H:i:s') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Creneaufin</th>
                <td>{% if creneau.creneauFin %}{{ creneau.creneauFin|date('H:i:s') }}{% endif %}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('creneau_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('creneau_edit', { 'id': creneau.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
